<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.Controller.CreateObjekatController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Object name" />
      <Label text="Object address" GridPane.rowIndex="2" />
      <Label text="Object type" GridPane.rowIndex="4" />
      <TextField fx:id="fldName" GridPane.rowIndex="1" />
      <TextField fx:id="fldAddress" GridPane.rowIndex="3" />
      <ComboBox fx:id="comboObjectType" editable="true" GridPane.rowIndex="5" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.rowIndex="6">
        <buttons>
          <Button defaultButton="true" mnemonicParsing="false" onAction="#okBtn" text="OK" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelBtn" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
