<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.Controller.CreateVlasnikController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="First name" />
      <TextField fx:id="fldFirstName" GridPane.rowIndex="1" />
      <Label text="Last name" GridPane.rowIndex="2" />
      <TextField fx:id="fldLastName" GridPane.rowIndex="3" />
      <Label text="JMBG" GridPane.rowIndex="4" />
      <TextField fx:id="fldJMBG" GridPane.rowIndex="5" />
      <Label text="Phone number" GridPane.rowIndex="6" />
      <Label text="E-mail" GridPane.rowIndex="8" />
      <TextField fx:id="fldPhoneNumber" GridPane.rowIndex="7" />
      <TextField fx:id="fldEmail" GridPane.rowIndex="9" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.rowIndex="10">
        <buttons>
          <Button defaultButton="true" mnemonicParsing="false" onAction="#createOwnerBtn" text="OK" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelBtn" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
